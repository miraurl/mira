{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  {
  "rewrites": [
    {
      "source": "/:shorten_code",
      "destination": "https://api.filin.is-app.top/:shorten_code"
    }
  ]
}
}
